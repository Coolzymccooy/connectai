{
  "name": "connectai---smb-ccaas-mvp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "worklog:endpoints": "node scripts/generate-endpoint-worklog.mjs",
    "prebuild": "npm run worklog:endpoints && node scripts/generate-version.mjs",
    "predev": "npm run worklog:endpoints",
    "predev:server": "npm run worklog:endpoints",
    "dev": "vite",
    "dev:server": "node server/index.js",
    "dev:worker": "node server/worker.js",
    "dev:all": "concurrently \"npm:dev:server\" \"npm:dev\"",
    "tauri:dev": "set \"PATH=%USERPROFILE%\\.cargo\\bin;%PATH%\" && tauri dev",
    "desktop:dev": "concurrently \"npm:dev:server\" \"npm:tauri:dev\"",
    "desktop:build": "set \"PATH=%USERPROFILE%\\.cargo\\bin;%PATH%\" && tauri build",
    "desktop:artifacts": "node scripts/package-desktop-release.mjs --repo Coolzymccooy/connectai",
    "desktop:release": "npm run desktop:build && npm run desktop:artifacts",
    "desktop:manifest:publish": "node scripts/publish-desktop-manifest.mjs",
    "tenant:migrate-local": "node scripts/migrate-default-tenant.mjs",
    "firebase:rules:deploy": "firebase deploy --only firestore:rules --project connectai-cec73",
    "test": "node --test",
    "build": "vite build",
    "preview": "vite preview",
    "load:test": "node scripts/load-test.mjs",
    "secrets:scan": "rg -n \"AIza|SG\\.|AC[0-9a-fA-F]{20,}|SK[0-9a-fA-F]{20,}|TWILIO_AUTH_TOKEN=|SENDGRID_API_KEY=|TERMII_API_KEY=|FIREBASE_SERVICE_ACCOUNT_JSON=\" -S . --glob \"!.env*\" --glob \"!node_modules/**\" --glob \"!dist/**\""
  },
  "dependencies": {
    "@google/genai": "^1.40.0",
    "@tauri-apps/api": "^2.10.1",
    "@twilio/voice-sdk": "^2.18.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "firebase": "^10.14.1",
    "firebase-admin": "^12.5.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.561.0",
    "ml-distance": "^4.0.1",
    "mongoose": "^9.1.6",
    "peer": "^1.0.2",
    "peerjs": "^1.5.5",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "recharts": "^3.6.0",
    "sip.js": "^0.21.2",
    "socket.io-client": "^4.8.3",
    "stripe": "^18.3.0",
    "twilio": "^5.12.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.10.0",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.24",
    "concurrently": "^9.0.1",
    "firebase-tools": "^15.5.1",
    "playwright": "^1.58.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
